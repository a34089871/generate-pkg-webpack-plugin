    "schema-utils": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/node": "^16.11.17",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/read-pkg-up": "^6.0.0",
    "@types/webpack": "^5.28.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
